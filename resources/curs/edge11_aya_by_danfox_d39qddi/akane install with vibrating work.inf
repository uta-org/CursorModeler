; xFree cursors for Windows XP.
; Win 2000/XP Alphablended Cursor Scheme installation file.

[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

; -- Common Information

[Scheme.Cur]
akane-arrow.cur
help.cur
akane-work-v.ani
akane-busy.ani
akane-text.cur
no.cur
akane-ns.cur
akane-ew.cur
akane-nwse.cur
akane-nesw.cur
akane-move.cur
akane-link.cur
akane-cross.cur
pen.cur
akane-alt.cur

[Scheme.Txt]
Readme.txt

[Strings]
CUR_DIR       = "Cursors\EDGE11"
SCHEME_NAME   = "Akane V"
pointer       = "akane-arrow.cur"
help          = "help.cur"
work          = "akane-work-v.ani"
busy          = "akane-busy.ani"
text          = "akane-text.cur"
unavailiable  = "no.cur"
vert          = "akane-ns.cur"
horz          = "akane-ew.cur"
dgn1          = "akane-nwse.cur"
dgn2          = "akane-nesw.cur"
move          = "akane-move.cur"
link          = "akane-link.cur"
cross         = "akane-cross.cur"
hand          = "pen.cur"
alternate     = "akane-alt.cur"
