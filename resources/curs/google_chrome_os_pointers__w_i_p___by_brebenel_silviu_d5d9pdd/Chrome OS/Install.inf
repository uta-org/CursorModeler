[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

[Scheme.Cur]
arrow.cur
help.cur
fleur.cur
busy.cur
text.cur
ns.cur
ew.cur
nwse.cur
nesw.cur
move.cur
link.cur
crosshair.cur
hand3.cur


[Strings]
CUR_DIR       = "Cursors\Chrome OS Cursors"
SCHEME_NAME   = "Chrome OS Cursor"
pointer       = "arrow.cur"
help          = "help.cur"
work          = "fleur.cur"
busy          = "busy.cur"
text          = "text.cur"
unavailiable  = "busy.cur"
vert          = "ns.cur"
horz          = "ew.cur"
dgn1          = "nwse.cur"
dgn2          = "nesw.cur"
move          = "move.cur"
link          = "link.cur"
cross         = "crosshair.cur"
hand          = "hand3.cur"
alternate     = "hand3.cur"