; xFree cursors for Windows XP.
; Win 2000/XP Alphablended Cursor Scheme installation file.

[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

; -- Common Information

[Scheme.Cur]
arrow-hikari.cur
help.cur
work-hikari.ani
busy-hikari.ani
text.cur
no-hikari.cur
ns-hikari.cur
ew-hikari.cur
nwse-hikari.cur
nesw-hikari.cur
move-hikari.cur
link.cur
cross.cur
pen.cur
alt-hikari.cur

[Scheme.Txt]
Readme.txt

[Strings]
CUR_DIR       = "Cursors\EDGE"
SCHEME_NAME   = "EDGE hikari"
pointer       = "arrow-hikari.cur"
help          = "help.cur"
work          = "work-hikari.ani"
busy          = "busy-hikari.ani"
text          = "text.cur"
unavailiable  = "no-hikari.cur"
vert          = "ns-hikari.cur"
horz          = "ew-hikari.cur"
dgn1          = "nwse-hikari.cur"
dgn2          = "nesw-hikari.cur"
move          = "move-hikari.cur"
link          = "link.cur"
cross         = "cross.cur"
hand          = "pen.cur"
alternate     = "alt-hikari.cur"