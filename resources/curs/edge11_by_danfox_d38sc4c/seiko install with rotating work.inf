; xFree cursors for Windows XP.
; Win 2000/XP Alphablended Cursor Scheme installation file.

[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

; -- Common Information

[Scheme.Cur]
seiko-arrow.cur
help.cur
seiko-work-r.ani
seiko-busy.ani
seiko-text.cur
no.cur
seiko-ns.cur
seiko-ew.cur
seiko-nwse.cur
seiko-nesw.cur
seiko-move.cur
seiko-link.cur
seiko-cross.cur
pen.cur
seiko-alt.cur

[Scheme.Txt]
Readme.txt

[Strings]
CUR_DIR       = "Cursors\EDGE11"
SCHEME_NAME   = "Seiko R"
pointer       = "seiko-arrow.cur"
help          = "help.cur"
work          = "seiko-work-r.ani"
busy          = "seiko-busy.ani"
text          = "seiko-text.cur"
unavailiable  = "no.cur"
vert          = "seiko-ns.cur"
horz          = "seiko-ew.cur"
dgn1          = "seiko-nwse.cur"
dgn2          = "seiko-nesw.cur"
move          = "seiko-move.cur"
link          = "seiko-link.cur"
cross         = "seiko-cross.cur"
hand          = "pen.cur"
alternate     = "seiko-alt.cur"