; xFree cursors for Windows XP.
; Win 2000/XP Alphablended Cursor Scheme installation file.

[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

; -- Common Information

[Scheme.Cur]
naomi-arrow.cur
help.cur
naomi-work-v.ani
naomi-busy.ani
naomi-text.cur
no.cur
naomi-ns.cur
naomi-ew.cur
naomi-nwse.cur
naomi-nesw.cur
naomi-move.cur
naomi-link.cur
naomi-cross.cur
pen.cur
naomi-alt.cur

[Scheme.Txt]
Readme.txt

[Strings]
CUR_DIR       = "Cursors\EDGE11"
SCHEME_NAME   = "Naomi V"
pointer       = "naomi-arrow.cur"
help          = "help.cur"
work          = "naomi-work-v.ani"
busy          = "naomi-busy.ani"
text          = "naomi-text.cur"
unavailiable  = "no.cur"
vert          = "naomi-ns.cur"
horz          = "naomi-ew.cur"
dgn1          = "naomi-nwse.cur"
dgn2          = "naomi-nesw.cur"
move          = "naomi-move.cur"
link          = "naomi-link.cur"
cross         = "naomi-cross.cur"
hand          = "pen.cur"
alternate     = "naomi-alt.cur"